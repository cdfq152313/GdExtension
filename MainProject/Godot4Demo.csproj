<Project Sdk="Godot.NET.Sdk/4.0.2">
    <PropertyGroup>
        <Nullable>enable</Nullable>
        <WarningsAsErrors>Nullable</WarningsAsErrors>
        <TargetFramework>net6.0</TargetFramework>
        <EnableDynamicLoading>true</EnableDynamicLoading>
    </PropertyGroup>
    <ItemGroup>
        <Content Include="RiderTestRunner\NetCoreRunner.tscn" />
    </ItemGroup>
    <ItemGroup>
        <ProjectReference Include="..\GodotSharpKit\GodotSharpKit.csproj" />
        <ProjectReference Include="..\GodotSharpKit.Generator\GodotSharpKit.Generator.csproj" OutputItemType="Analyzer" ReferenceOutputAssembly="false" />
    </ItemGroup>
    <ItemGroup>
        <CompilerVisibleItemMetadata Include="AdditionalFiles" MetadataName="Container" />
        <CompilerVisibleItemMetadata Include="AdditionalFiles" MetadataName="Class" />
        <AdditionalFiles Include="*.tscn" />
        <AdditionalFiles Include="Inner\*.tscn" Container="A" Class="PackedScene" />
        <AdditionalFiles Include="Deep\Deep2\*.tscn" Container="A" Class="PackedScene" />
        <AdditionalFiles Include="icon.svg" Container="Image" />
    </ItemGroup>
</Project>